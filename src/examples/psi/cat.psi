#!/usr/bin/psylla
/##
cat.psi — print files to standard output

Synopsis:
	cat.psi [FILE]…
##/

arguments isempty { arguments '-' append } if

arguments
{
	dup '-' ne { filereader } { stdin } ifelse
	/in exch def

	{
		in 4096 readstring
		exch print
		not { exit } if
	}
	loop
	stdout flush
}
bind forall

