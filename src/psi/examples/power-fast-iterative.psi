#!/usr/bin/psi

/power
{
	dict begin
		/n exch def
		/x exch def
		/p 1 def
		{
			n iszero { exit } if
			n 2 mod 0 ne
			{
				/p p x mul def
				/n n 1 sub def
			}
			if
			/n n 2 idiv def
			/x x dup mul def
		}
		loop
		p
	end
}
bind def

arguments 0 get tointeger
arguments 1 get tointeger
power tostring say
