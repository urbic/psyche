#!/usr/bin/psyche

/lines array def

/collectlines
{
	{
		dup readline
		{ lines exch append } { exit } ifelse
	}
	loop
}
bind def

arguments { filereader collectlines } forall

arguments isempty { stdin collectlines } if

lines { 2 { length exch } repeat cmp } bind sort { print } bind forall

stdout flush
